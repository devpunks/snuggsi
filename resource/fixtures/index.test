const
  { test }
      = require ('snuggsi')
  , fixture
      = new (require ('./index.es'))
  , context = {}


test `.get`

(assert => {
  fixture.get (context)

  assert ( context.status == 200 )
})


test `.put`

(assert => {
  fixture.put (context)

  assert ( context.status == 200 )
})

test ('.post', t => {
  fixture.post (context)

  t.equal (200, context.status )
  t.end ()
})

test ('.patch', t => {
  fixture.patch (context)

  t.equal (200, context.status )
  t.end ()
})

test ('.delete', t => {
  fixture.delete (context)

  t.equal (200, context.status )
  t.end ()
})
