//import { test, expect } from 'node:test';
const n = require('node:test')
, it = n.it // test
, test = n.test
, mock = n.mock
, skip = it.skip
, todo = it.todo
, only = it.only
, after = n.afterEach
, before = n.beforeEach
, describe = n.describe
, assert = require('assert')

// https://nodejs.org/api/test.html#class-suitecontext
describe('watch', t => {
  console.log(t)
  console.log(it)

  test('should add two numbers', _ => {
    const result = 1 + 2
    console.log(_.name)
    assert.strictEqual(result, 3)
  })

  it.only('it should equal test', _ => {
    console.log(_.name)
    assert.strictEqual(test.it, it)
  })
})

describe.todo( 'TODO', _ => todo('TODO: something') )
