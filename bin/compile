#!/usr/bin/env bash

START=` date +%s `

echo
echo "🎁  Compilation Process Started …"
echo

bin/bundle \
# && bin/transpile \
# && bin/shrink    \
# && bin/compress

FINISH=` date +%s `

echo
echo "🎁  Compilation Process Finished"
echo
echo "  ⏰  Started  👉  `date -r $START`"
echo "  🏁  Finished 👉  `date -r $FINISH`"
echo "  🕓  Total compilation duration 👉  $((FINISH-START)) secs."
echo
