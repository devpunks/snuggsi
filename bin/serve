#!/usr/bin/env bash

export ROOT=$(pwd)
export PORT=8181

, polyfills =
    `${root}/polyfills`

, server =
    `${root}/server`

process.env.Node_Path
  = [ root, process.env.NODE_PATH ].join `:`


console.warn (require (polyfills))
console.warn (require (server))
// --expose-http2 \ # Experimental HTTP/2 flag https://nodejs.org/api/cli.html#cli_expose_http2
