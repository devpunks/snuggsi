const
  { test, serve }
    = require ('test')

const
  puppeteer
    = require ('puppeteer')


test ('new HTMLElement', async t => {

  const
    server
      = serve ``
  , browser
      = await puppeteer.launch ()
  , page
      = await browser.newPage ()


  await page
    .goto ('https://example.com')

//await page.content ()

  console.warn (page)

  await page
    .screenshot ({ path: 'screenshot.png' })

  server.close ``
  t.end ()
})

