const
  { test, fetch, read }
    = require ('test')

, { route }
    = require ('.')

, { Server }
    = require ('..')


test ('route URI to middleware', async t => {

  const
    server
      = (new Server).serve ``

  , response
      = await fetch ('http://localhost:8181/foo/')

  , body
      = await response.body

  , status = response.status


  console.log (status, await read (body))

  t.ok (true)

  server.close ``
  t.end ()
})
