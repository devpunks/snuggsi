{
  "name": "snuggsi",
  "license": "MIT",
  "version": "2024.12.18",
  "contributors": [],
  "author": [
    "Snuggs",
    "Brandondees"
  ],
  "keywords": [
    "web",
    "components",
    "custom",
    "elements"
  ],
  "description": "Easy Web Components in ~1kB",
  "homepage": "https://github.com/devpunks/snuggsi#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/devpunks/snuggsi.git"
  },
  "bugs": {
    "url": "https://github.com/devpunks/snuggsi/issues"
  },
  "workspaces": [],
  "main": "index.js",
  "browser": "index.js",
  "files": [
    "/dist/*.min*"
  ],
  "packageManager": "npm@9.5.1",
  "module": "./dist/snuggsi.min.es",
  "unpkg": "./dist/snuggsi.min.es.js",
  "config": {
    "PORT": "4321"
  },
  "exports": {
    ".": {
      "browser": {
        "default": "./dist/snuggsi.min.es.js"
      }
    }
  },
  "cpu": [
    "x64",
    "arm64"
  ],
  "os": [
    "win32",
    "linux",
    "darwin",
    "android"
  ],
  "engines": {
    "npm": ">=9.x",
    "node": ">=18.x"
  },
  "directories": {
    "bin": "bin",
    "example": "examples"
  },
  "bin": {
    "snuggsi": "bin/snuggsi"
  },
  "watch": [
    ".",
    "watch",
    "token-list",
    "html-element",
    "html-template-element"
  ],
  "bundleDependencies": [],
  "dependencies": {
    "basic-auth": "^2.0.1",
    "koa": "^2.15.3",
    "koa-cors": "^0.0.16",
    "koa-static": "^5.0.0"
  },
  "devDependencies": {
    "buble": "^0.20.0",
    "jsdom": "^25.0.1",
    "nyc": "^17.1.0",
    "puppeteer-core": "^23.11.1",
    "standard": "^17.1.2",
    "uglify-es": "^3.3.9"
  },
  "scripts": {
    "start": "snuggsi serve",
    "cover": "snuggsi cover",
    "watch": "snuggsi watch",
    "install": "echo  Installing snuggsi",
    "postinstall": "echo Post install linking",
    "preinstall": "echo  pre-Installing snuggsi",
    "preversion": "echo 'npm preversion (test)'",
    "version": "echo 'npm version (build)' # git add -A dist",
    "postversion": "echo 'npm postversion (publish)' # git push && git push --tags",
    "test": "node .{/*,}/*.test.{js,es} && npm run cover"
  }
}
