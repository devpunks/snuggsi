{ "name": "snuggsi",
  "private": false,
  "type": "module",
  "license": "MIT",
  "version": "2022.1.2-1",
  "author": [
    "Snuggs",
    "Brandondees"
  ],
  "description": "Easy Web Components in 1kB",
  "homepage": "https://github.com/devpunks/snuggsi#readme",
  "repository": {
    "type": "git",
    "url": "https://github.com/devpunks/snuggsi"
  },
  "bugs": {
    "url": "https://github.com/devpunks/snuggsi/issues"
  },
  "engines": {
    "npm": ">=9.x",
    "node": ">=18.x"
  },
  "keywords": [
    "web",
    "components"
  ],
  "main": "public/index.js",
  "exports": {
    ".": {
      "browser": {
        "default": "./dist/snuggsi.min.es.js"
      }
    }
  },
  "unpkg": "./dist/snuggsi.min.es.js",
  "module": "./dist/snuggsi.min.es",
  "directories": {
    "bin": "bin",
    "example": "examples"
  },
  "bin": {
    "snuggsi": "bin/snuggsi"
  },
  "scripts": {
    "test": "snuggsi test",
    "start": "snuggsi serve",
    "package": "snuggsi package",
    "postinstall": "#snuggsi publish"
  },
  "devDependencies": {
    "nyc": "*",
    "jsdom": "*",
    "snuggsi": ".",
    "standard": "*",
    "buble": "^0.20",
    "uglify-es": "*",
    "puppeteer-core": "*"
  },
  "dependencies": {
    "koa": "*",
    "koa-cors": "*",
    "koa-static": "*",
    "basic-auth": "*"
  }
}
